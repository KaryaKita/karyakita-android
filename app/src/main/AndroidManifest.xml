<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.karyakita.karyakita_android_new">


    <uses-permission android:name="android.permission.INTERNET" />

    <!-- To auto-complete the email text field in the activity_login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_main_app_karyakita"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_main_app_karyakita"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".customer.SplashActivity"
            android:theme="@style/AppTheme.SplashTheme"/>

        <activity android:name=".customer.home.HomeCustomerActivity">
        </activity>
        <activity android:name=".example.MovieActivity">


        </activity>
        <activity android:name=".register_as.RegisterAsActivity">

        </activity>

        <activity android:name=".register.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>